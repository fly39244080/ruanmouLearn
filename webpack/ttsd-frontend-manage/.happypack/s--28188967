'use strict';

var commonFun = require('publicJs/commonFun');

var $downTime = $('#downTime');

commonFun.countDownLoan({
    btnDom: $downTime,
    time: 5,
    isAfterText: '',
    textCounting: 's'
}, function () {
    window.location = $downTime.data('redirect');
});

$('#okButton').click(function () {
    window.location = $downTime.data('redirect');
});